{
  "name": "full-stack-open",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:test": "cross-env NODE_ENV=test node server/index.js",
    "start:react": "react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand --detectOpenHandles",
    "test:react": "react-scripts test --coverage",
    "test:e2e": "cypress run",
    "eject": "react-scripts eject",
    "server": "json-server -p3001 --watch server/db.json",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js",
    "lint": "eslint .",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.9.1",
    "morgan": "^1.10.0",
    "prop-types": "^15.8.1",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.3.2",
    "cypress": "^12.5.1",
    "deep-freeze": "^0.0.1",
    "eslint": "^8.33.0",
    "eslint-plugin-cypress": "^2.12.1",
    "jest": "^27.5.1",
    "json-server": "^0.17.1",
    "nodemon": "^2.0.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "supertest": "^6.3.3",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001"
}
